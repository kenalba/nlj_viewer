    {
      "id": "goal_product_knowledge_001",
      "name": "Demonstrate comprehensive product knowledge during test drive",
      "description": "Sales consultant successfully demonstrates technical knowledge, feature benefits, and competitive positioning during the Ioniq 9 test drive experience",
      "script": "if Outcome==1 then\nmessage=\"Excellent! You demonstrated comprehensive product knowledge and effectively addressed all customer concerns during the test drive.\"\ncelebrate=true\nendif\n\nif Outcome==2 then\nmessage=\"Good work! You showed solid product knowledge but could improve in some areas to build stronger customer confidence.\"\ncelebrate=true\nendif\n\nif Outcome==3 then\nmessage=\"Focus on developing stronger product knowledge. Customers need specific, accurate information to build confidence in both the vehicle and your expertise.\"\ncelebrate=false\nendif\n"
    }
  ]
